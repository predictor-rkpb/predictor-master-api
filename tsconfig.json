{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "paths": {
      "shared-lib": [
        "./libs/shared-lib/index"
      ],
      "express-extensions": [
        "./libs/express-shared-lib/index"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}